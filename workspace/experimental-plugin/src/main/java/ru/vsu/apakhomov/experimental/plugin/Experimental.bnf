{
  parserClass="ru.vsu.apakhomov.experimental.plugin.parser.ExperimentalParser"

  extends="com.intellij.extapi.psi.ASTWrapperPsiElement"

  psiClassPrefix="Experimental"
  psiImplClassSuffix="Impl"
  psiPackage="ru.vsu.apakhomov.experimental.plugin.psi"
  psiImplPackage="ru.vsu.apakhomov.experimental.plugin.psi.impl"

  elementTypeHolderClass="ru.vsu.apakhomov.experimental.plugin.psi.ExperimentalTypes"
  elementTypeClass="ru.vsu.apakhomov.experimental.plugin.psi.ExperimentalElementType"
  tokenTypeClass="ru.vsu.apakhomov.experimental.plugin.psi.ExperimentalTokenType"

  psiImplUtilClass="ru.vsu.apakhomov.experimental.plugin.psi.impl.ExperimentalPsiImplUtil"
}

simpleFile ::= item_*

private item_ ::= (property|COMMENT|CRLF)

property ::= (KEY? SEPARATOR VALUE?) | KEY {mixin="ru.vsu.apakhomov.experimental.plugin.psi.impl.ExperimentalNamedElementImpl"
implements="ru.vsu.apakhomov.experimental.plugin.psi.ExperimentalNamedElement" methods=[getKey getValue getName setName getNameIdentifier getPresentation]}